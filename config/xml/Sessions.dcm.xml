<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Sessions" table="sessions">
    <id name="sid" type="string" column="sid" length="128"/>
    <id name="ssid" type="string" column="ssid" length="128"/>
    <field name="uid" type="integer" column="uid" nullable="false"/>
    <field name="hostname" type="string" column="hostname" length="128" nullable="false"/>
    <field name="timestamp" type="integer" column="timestamp" nullable="false"/>
    <field name="cache" type="integer" column="cache" nullable="false"/>
    <field name="session" type="blob" column="session" nullable="true"/>
  </entity>
</doctrine-mapping>
