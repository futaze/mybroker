<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TaxonomyIndex" table="taxonomy_index">
    <id name="nid" type="integer" column="nid">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="tid" type="integer" column="tid" nullable="false"/>
    <field name="sticky" type="boolean" column="sticky" nullable="true"/>
    <field name="created" type="integer" column="created" nullable="false"/>
  </entity>
</doctrine-mapping>
