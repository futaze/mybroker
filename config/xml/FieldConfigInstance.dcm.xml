<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="FieldConfigInstance" table="field_config_instance">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fieldId" type="integer" column="field_id" nullable="false"/>
    <field name="fieldName" type="string" column="field_name" length="32" nullable="false"/>
    <field name="entityType" type="string" column="entity_type" length="32" nullable="false"/>
    <field name="bundle" type="string" column="bundle" length="128" nullable="false"/>
    <field name="data" type="blob" column="data" nullable="false"/>
    <field name="deleted" type="boolean" column="deleted" nullable="false"/>
  </entity>
</doctrine-mapping>
