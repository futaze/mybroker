<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CacheBlock" table="cache_block">
    <id name="cid" type="string" column="cid" length="255">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="data" type="blob" column="data" nullable="true"/>
    <field name="expire" type="integer" column="expire" nullable="false"/>
    <field name="created" type="integer" column="created" nullable="false"/>
    <field name="serialized" type="smallint" column="serialized" nullable="false"/>
  </entity>
</doctrine-mapping>
