<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Actions" table="actions">
    <id name="aid" type="string" column="aid" length="255">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="type" type="string" column="type" length="32" nullable="false"/>
    <field name="callback" type="string" column="callback" length="255" nullable="false"/>
    <field name="parameters" type="blob" column="parameters" nullable="false"/>
    <field name="label" type="string" column="label" length="255" nullable="false"/>
  </entity>
</doctrine-mapping>
