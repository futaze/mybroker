<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="DateFormats" table="date_formats">
    <id name="dfid" type="integer" column="dfid">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="format" type="string" column="format" length="100" nullable="false"/>
    <field name="type" type="string" column="type" length="64" nullable="false"/>
    <field name="locked" type="boolean" column="locked" nullable="false"/>
  </entity>
</doctrine-mapping>
