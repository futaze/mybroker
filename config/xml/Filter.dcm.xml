<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Filter" table="filter">
    <id name="format" type="string" column="format" length="255"/>
    <id name="name" type="string" column="name" length="32"/>
    <field name="module" type="string" column="module" length="64" nullable="false"/>
    <field name="weight" type="integer" column="weight" nullable="false"/>
    <field name="status" type="integer" column="status" nullable="false"/>
    <field name="settings" type="blob" column="settings" nullable="true"/>
  </entity>
</doctrine-mapping>
