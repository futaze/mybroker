<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Flood" table="flood">
    <id name="fid" type="integer" column="fid">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="event" type="string" column="event" length="64" nullable="false"/>
    <field name="identifier" type="string" column="identifier" length="128" nullable="false"/>
    <field name="timestamp" type="integer" column="timestamp" nullable="false"/>
    <field name="expiration" type="integer" column="expiration" nullable="false"/>
  </entity>
</doctrine-mapping>
