<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SearchIndex" table="search_index">
    <id name="word" type="string" column="word" length="50"/>
    <id name="sid" type="integer" column="sid"/>
    <id name="type" type="string" column="type" length="16"/>
    <field name="score" type="float" column="score" nullable="true"/>
  </entity>
</doctrine-mapping>
