<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Node" table="node">
    <id name="nid" type="integer" column="nid">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="vid" type="integer" column="vid" nullable="true"/>
    <field name="type" type="string" column="type" length="32" nullable="false"/>
    <field name="language" type="string" column="language" length="12" nullable="false"/>
    <field name="title" type="string" column="title" length="255" nullable="false"/>
    <field name="uid" type="integer" column="uid" nullable="false"/>
    <field name="status" type="integer" column="status" nullable="false"/>
    <field name="created" type="integer" column="created" nullable="false"/>
    <field name="changed" type="integer" column="changed" nullable="false"/>
    <field name="comment" type="integer" column="comment" nullable="false"/>
    <field name="promote" type="integer" column="promote" nullable="false"/>
    <field name="sticky" type="integer" column="sticky" nullable="false"/>
    <field name="tnid" type="integer" column="tnid" nullable="false"/>
    <field name="translate" type="integer" column="translate" nullable="false"/>
  </entity>
</doctrine-mapping>
