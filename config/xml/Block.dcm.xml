    <field name="module" type="string" column="module" length="64" nullable="false"/>
    <field name="delta" type="string" column="delta" length="32" nullable="false"/>
    <field name="theme" type="string" column="theme" length="64" nullable="false"/>
    <field name="status" type="boolean" column="status" nullable="false"/>
    <field name="weight" type="integer" column="weight" nullable="false"/>
    <field name="region" type="string" column="region" length="64" nullable="false"/>
    <field name="custom" type="boolean" column="custom" nullable="false"/>
    <field name="visibility" type="boolean" column="visibility" nullable="false"/>
    <field name="pages" type="text" column="pages" nullable="false"/>
    <field name="title" type="string" column="title" length="64" nullable="false"/>
    <field name="cache" type="boolean" column="cache" nullable="false"/>
  </entity>
</doctrine-mapping>
